{
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "rules": {
      "object-curly-spacing": 0,
      "new-cap": 0,
      "space-before-function-paren": 0,
      "linebreak-style": ["error", "windows"],
      "indent": ["error", 4]
    }
  }
  